{
  "name": "fatchrono",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.9.0",
  "workspaces": [
    "apps/backend",
    "apps/web",
    "packages/shared"
  ],
  "scripts": {
    "dev:backend": "npm run start:dev --workspace @fatchrono/backend",
    "dev:web": "npm run dev --workspace @fatchrono/web",
